{
  "name": "st-nucleo-f091rc-gcc",
  "version": "0.0.3-tri-0.8.0",
  "description": "Official mbed build target for the mbed ST Nucleo f091rc development board, using armgcc toolchain",
  "homepage": "https://github.com/ARMmbed/target-st-nucleo-f091rc-gcc",
  "repository": {
    "url": "git@github.com:ARMmbed/target-st-nucleo-f091rc-gcc.git",
    "type": "git"
  },
  "licenses": [
    {
      "url": "https://spdx.org/licenses/Apache-2.0",
      "type": "Apache-2.0"
    }
  ],
  "inherits": {
    "mbed-gcc": "tridonic-com/target-mbed-gcc#b1.2.2-tri-0.8.0"
  },
  "keywords": [
    "mbed-target:nucleo-f091rc",
    "mbed-official",
    "nucleo-f091rc",
    "nucleo",
    "x-nucleo",
    "ST",
    "STM",
    "gcc"
  ],
  "similarTo": [
    "nucleo-f091rc",
    "f091rc",
    "f091",
    "nucleo",
    "stm32f091rct6",
    "stm32f091rc",
    "stm32f091xc",
    "stm32f091",
    "stm32f0",
    "stm32",
    "st",
    "cortex-m0",
    "armv7-m",
    "arm",
    "gcc",
    "mbed",
    "stm32f091rc-no-inherit",
    "stm32f091xc-no-inherit"
  ],
  "config": {
    "minar": {
      "initial_event_pool_size": 20,
      "additional_event_pools_size": 50,
      "platform_time_mask": "0xFFFFu"
    },
    "mbed-os": {},
    "cmsis": {
      "nvic": {
        "ram_vector_address": "0x20000000",
        "flash_vector_address": "0x08005080",
        "user_irq_offset": 16,
        "user_irq_number": 107,
        "has_custom_vtor": 1
      }
    },
    "hardware": {
      "externalClock": "8000000",
      "internalClock": "8000000",
      "pins": {
        "LED1": "PA_5",
        "LED2": "NC",
        "LED3": "NC",
        "LED4": "NC",
        "USER_BUTTON": "PC_13",
        "SW2": "NC",
        "SW3": "NC",
        "SERIAL_TX": "PA_2",
        "SERIAL_RX": "PA_3",
        "USBTX": "PA_2",
        "USBRX": "PA_3",
        "STDIO_UART_TX": "PA_2",
        "STDIO_UART_RX": "PA_3",
        "STDIO_UART": "USART2_BASE",
        "D0": "PA_3",
        "D1": "PA_2",
        "D2": "PA_10",
        "D3": "PB_3",
        "D4": "PB_5",
        "D5": "PB_4",
        "D6": "PB_10",
        "D7": "PA_8",
        "D8": "PA_9",
        "D9": "PC_7",
        "D10": "PB_6",
        "D11": "PA_7",
        "D12": "PA_6",
        "D13": "PA_5",
        "D14": "PB_9",
        "D15": "PB_8",
        "I2C_SCL": "PB_8",
        "I2C_SDA": "PB_9",
        "SPI_MOSI": "PA_7",
        "SPI_MISO": "PA_6",
        "SPI_SCK": "PA_5",
        "SPI_CS": "PA_4",
        "PWM_OUT": "PB_3"
      }
    }
  },
  "toolchain": "CMake/toolchain.cmake",
  "debug": [
    "valinor",
    "--target",
    "stm32f091rc",
    "$program"
  ],
  "scripts": {
    "debug": [
      "valinor",
      "--target",
      "ST_NUCLEO_F091RC",
      "$program"
    ],
    "test": [
      "mbed_test_wrapper",
      "--target",
      "ST_NUCLEO_F091RC",
      "$program"
    ]
  }
}
